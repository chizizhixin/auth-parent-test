<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auth.system.mapper.AppletUserMapper">
    <update id="closePhone">
        UPDATE applet_user
        SET `phone` = NULL
        WHERE id = #{id};
    </update>
</mapper>
